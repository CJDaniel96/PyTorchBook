   "source": [
    "v = np.array([2, 1])\n",
    "s = np.array([-3, 2])\n",
    "\n",
    "vMag = np.linalg.norm(v)\n",
    "sMag = np.linalg.norm(s)\n",
    "\n",
    "cos = v @ s / (vMag * sMag)\n",
    "\n",
    "theta = math.degrees(math.acos(cos))\n",
    "\n",
    "print(theta)"
   ]
